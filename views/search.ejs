var template_search = `

<%


item = item['@graph'][0];

%>

<div class="row">

<div class="col s12">	



<!-- data feed items -->
	<% for (var i in item.dataFeedElement) { %>
		<div class="card-panel z-depth-1">
			<div class="row valign-wrapper">
            	<div class="col s2">
            		<img  class="z-depth-2" style="background:white;width:100px;height:100px;object-fit: cover;object-position: 50% 50%;" src="image/php?qid=<%= item.dataFeedElement[i]['@id'].replace('http://www.wikidata.org/entity/', '') %>">
				</div>
				<div class="col s10">
					<span style="font-size:1.5em;">
					<a href="?id=<%= item.dataFeedElement[i]['@id'].replace('http://www.wikidata.org/entity/', '') %>">
						<%= get_literal(item.dataFeedElement[i].name) %>
					</a>
					</span>				
				
					<p>
					<% if (item.dataFeedElement[i].description) { 
						var description = get_literal(item.dataFeedElement[i].description);
						if (description.length > 512) {
							description = description.substring(0, 512)  + '...';
						}
					%>
						<%=  description %>
					<% } %>
					</p>
				</div>				
			</div>
		</div>
		
	<% } %>

</div>
</div>



`;